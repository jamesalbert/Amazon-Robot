<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Frame" id="Main">
    <property name="height">200</property>
    <property name="width">600</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
      <rows>
        <row id="0">
          <property name="minsize">0</property>
          <property name="pad">15</property>
        </row>
        <row id="1">
          <property name="pad">15</property>
          <property name="weight">0</property>
        </row>
        <row id="2">
          <property name="pad">15</property>
        </row>
        <row id="3">
          <property name="pad">15</property>
        </row>
        <row id="4">
          <property name="pad">15</property>
        </row>
        <row id="5">
          <property name="pad">15</property>
        </row>
        <row id="6">
          <property name="pad">15</property>
        </row>
        <row id="7">
          <property name="pad">15</property>
        </row>
        <row id="8">
          <property name="pad">15</property>
        </row>
      </rows>
    </layout>
    <child>
      <object class="ttk.Label" id="email_label">
        <property name="font">{Verdana} 10 {}</property>
        <property name="text" translatable="yes">* email: </property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="email">
        <property name="font">{Verdana} 10 {}</property>
        <property name="takefocus">true</property>
        <property name="textvariable">string:email</property>
        <property name="width">40</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="password_label">
        <property name="font">{Verdana} 10 {}</property>
        <property name="text" translatable="yes">* password: </property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="password">
        <property name="font">{Verdana} 10 {}</property>
        <property name="show">•</property>
        <property name="textvariable">string:password</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="service_user_id_label">
        <property name="font">{Verdana} 10 {}</property>
        <property name="text" translatable="yes">* sUserID</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="service_user_id">
        <property name="font">{Verdana} 10 {}</property>
        <property name="textvariable">string:service_user_id</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="service_password_label">
        <property name="font">{Verdana} 10 {}</property>
        <property name="text" translatable="yes">* sPassword</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">4</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="service_password">
        <property name="font">{Verdana} 10 {}</property>
        <property name="show">•</property>
        <property name="textvariable">string:service_password</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">4</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="cc_label">
        <property name="font">{Verdana} 10 {}</property>
        <property name="text" translatable="yes">card number: </property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">5</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="card_number">
        <property name="font">{Verdana} 10 {}</property>
        <property name="textvariable">string:card_number</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">5</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="card_name_label">
        <property name="font">{Verdana} 10 {}</property>
        <property name="text" translatable="yes">name on card: </property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">6</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="card_name">
        <property name="font">{Verdana} 10 {}</property>
        <property name="textvariable">string:card_name</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">6</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="expiration_label">
        <property name="font">{Verdana} 10 {}</property>
        <property name="text" translatable="yes">expiration date: </property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">7</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Combobox" id="exp_month">
        <property name="textvariable">int:exp_month</property>
        <property name="values">01 02 03 04 05 06 07 08 09 10 11 12</property>
        <property name="width">2</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">7</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Combobox" id="exp_year">
        <property name="textvariable">int:exp_year</property>
        <property name="values">2015 2016 2017 2018 2019 2020 2021 2022 2023 2024 2025 2026 2027 2028 2029 2030 2031 2032 2033 2034 2035 2036</property>
        <property name="width">4</property>
        <layout>
          <property name="column">1</property>
          <property name="padx">45</property>
          <property name="propagate">True</property>
          <property name="row">7</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="csc_label">
        <property name="font">{Verdana} 10 {}</property>
        <property name="text" translatable="yes">security code: </property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">8</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="csc">
        <property name="font">{Verdana} 10 {}</property>
        <property name="textvariable">string:csc</property>
        <property name="width">3</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">8</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="save">
        <property name="command">save_preferences</property>
        <property name="default">active</property>
        <property name="text" translatable="yes">save</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">9</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="required">
        <property name="font">{Verdana} 10 {}</property>
        <property name="text" translatable="yes">*: required field</property>
        <layout>
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">9</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
